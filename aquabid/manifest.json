{
    "manifest_version": 2,
    "name": "aquabid",
    "short_name": "aquabid",
    "description": "aquabid extension",
    "version": "0.0.1",
    "minimum_chrome_version": "59",
    
    "content_scripts": [{
      "matches": ["http://www.aquabid.com/*"],
      "all_frames": true,
      "css": ["styles.css"],
      "js": ["aquabid.js"]
    }],
    
    "permissions": [
        "tabs"
    ]
}
